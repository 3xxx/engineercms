function CreateBlockTable() {
    //返回控件的数据库对象
    var database =mxOcx.GetDatabase();
    //返回数据库中的块表对象
    var mxblocktable = database.GetBlockTable();
    //增加新的块表
    var blocktablestr = mxblocktable.Add("myblocktable");
   /* //设置样式
    blocktablestr.SetDimVarDouble(40, 20.0);
